#################################################################
#    OBJECT PROPERTIES - SIS Integration (Numeric Payloads)
#################################################################

###  https://ptb.de/dcc/ont/hasQuantityValue
dcc:hasQuantityValue rdf:type owl:ObjectProperty ;
    rdfs:domain dcc:Quantity ;
    rdfs:range sis:QuantityValue ;
    skos:prefLabel "has quantity value"@en ,
                   "hat Größenwert"@de ;
    skos:definition """Links a DCC Quantity to its SIS numeric payload.
This is the primary bridge between DCC structure and SIS math layer.
The range includes sis:Real, sis:Complex, sis:Constant, sis:RealList, etc."""@en ;
    rdfs:comment "Delegates numeric representation to SIS ontology for GUM compliance."@en .


###  https://ptb.de/dcc/ont/hasSIReal
dcc:hasSIReal rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf dcc:hasQuantityValue ;
    rdfs:domain dcc:Quantity ;
    rdfs:range sis:Real ;
    skos:prefLabel "has SI real value"@en ,
                   "hat SI-Realwert"@de ;
    skos:exactMatch six:real .


###  https://ptb.de/dcc/ont/hasSIComplex
