#################################################################
#    Object Properties
#################################################################

###  https://ptb.de/sis/hasComplexInList
sis:hasComplexInList rdf:type owl:ObjectProperty ;
                     rdfs:domain sis:ComplexList ;
                     rdfs:range sis:ComplexInList ;
                     skos:prefLabel "contains the complex element"@en ,
                                    "enthält das komplexe Element"@de .


###  https://ptb.de/sis/hasCovariance
sis:hasCovariance rdf:type owl:ObjectProperty ;
                  rdfs:domain sis:CovarianceMatrix ;
                  rdfs:range sis:Covariance ;
                  skos:prefLabel "has covariance element"@en ,
                                 "hat Kovarianzelement"@de .


###  https://ptb.de/sis/hasCovarianceMatrix
sis:hasCovarianceMatrix rdf:type owl:ObjectProperty ;
                        rdfs:domain sis:MeasurementUncertaintyMultivariate ;
                        rdfs:range sis:CovarianceMatrix ;
                        skos:prefLabel "has covariance matrix"@en ,
                                       "hat Kovarianzmatrix"@de .


###  https://ptb.de/sis/hasCoverageIntervalMU
sis:hasCoverageIntervalMU rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf sis:hasMeasurementUncertaintyUnivariate ;
                          rdfs:domain sis:QuantityValue ;
                          rdfs:range sis:CoverageIntervalMU ;
                          skos:prefLabel "has coverage interval"@en ,
                                         "hat Überdeckungsintervall"@de .


###  https://ptb.de/sis/hasExpandedMU
sis:hasExpandedMU rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sis:hasMeasurementUncertaintyUnivariate ;
                  rdfs:domain sis:QuantityValue ;
                  rdfs:range sis:ExpandedMU ;
                  skos:prefLabel "has expanded uncertainty"@en ,
                                 "hat erweiterte Messunsicherheit"@de .


###  https://ptb.de/sis/hasListInList
sis:hasListInList rdf:type owl:ObjectProperty ;
                  rdfs:domain sis:ListList ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( sis:ComplexList
                                             sis:ListList
                                             sis:RealList
                                            ) 
                              ] ;
                   skos:prefLabel "contains the list-like element"@en ,
                                  "enthält das listenartige Element"@de .


###  https://ptb.de/sis/hasMeasurementUncertainty
sis:hasMeasurementUncertainty rdf:type owl:ObjectProperty ;
                              rdfs:domain sis:QuantityValue ;
                              rdfs:range sis:MeasurementUncertainty ;
                              skos:prefLabel "has uncertainty declaration"@en ,
                                             "hat Unsicherheitsdeklaration"@de .


###  https://ptb.de/sis/hasListMeasurementUncertaintyUnivariate
sis:hasListMeasurementUncertaintyUnivariate rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf sis:hasMeasurementUncertainty ;
                                            rdfs:domain sis:RealList ;
                                            rdfs:range sis:ListMeasurementUncertaintyUnivariate .


###  https://ptb.de/sis/hasMeasurementUncertaintyMultivariate
sis:hasMeasurementUncertaintyMultivariate rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf sis:hasMeasurementUncertainty ;
                                          rdfs:domain [ rdf:type owl:Class ;
                                                        owl:unionOf ( sis:Complex
                                                                      sis:RealList
                                                                    )
                                                      ] ;
                                          rdfs:range sis:MeasurementUncertaintyMultivariate ;
                                          skos:prefLabel "has multivariate measurement uncertainty"@en ,
                                                         "hat multivariate Messunsicherheit"@de .


###  https://ptb.de/sis/hasMeasurementUncertaintyUnivariate
sis:hasMeasurementUncertaintyUnivariate rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf sis:hasMeasurementUncertainty ;
                                        rdfs:domain [ rdf:type owl:Class ;
                                                      owl:unionOf ( sis:Real
                                                                    sis:RealInList
                                                                  )
                                                    ] ;
                                        rdfs:range sis:MeasurementUncertaintyUnivariate ;
                                        skos:prefLabel "has univariate measurement uncertainty"@en ,
                                                       "hat univariate Messunsicherheit"@de .


###  https://ptb.de/sis/hasQuantityType
sis:hasQuantityType rdf:type owl:ObjectProperty ;
                    rdfs:comment """Standardized kind of quantity associated to a quantity value,
                                    expressed as the identifier of a term from a controlled terminology.
                                    We recommend choosing high-level terms (energy, length) from the SI reference point
                                    and more specific terms (electric energy, enthalpy, diameter, depth) from QUDT."""@en ;
                    rdfs:domain sis:QuantityValue ;
                    rdfs:range sis:QuantityType ;
                    skos:prefLabel "has kind of quantity"@en , "hat Größenart"@de .


###  https://ptb.de/sis/hasRealInList
sis:hasRealInList rdf:type owl:ObjectProperty ;
                  rdfs:domain sis:RealList ;
                  rdfs:range sis:RealInList ;
                  skos:prefLabel "contains the real element"@en ,
                                 "enthält das reelle Element"@de .


###  https://ptb.de/sis/hasSIMeasurementUnit
sis:hasSIMeasurementUnit rdf:type owl:ObjectProperty ;
                         rdfs:comment """SI compatible measurement unit associated to a quantity,
                                         expressed as the identifier of a term from a controlled terminology.
                                         We recommend choosing terms from the SI reference point."""@en ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( sis:Complex
                                                     sis:Constant
                                                     sis:Covariance
                                                     sis:Real
                                                     sis:RealInList
                                                     sis:RealList
                                                   )
                                     ] ;
                         rdfs:range sirp:MeasurementUnit ;
                         skos:prefLabel "has SI measurement unit"@en , "hat SI Messeinheit"@de .


###  https://ptb.de/sis/hasStandardMU
sis:hasStandardMU rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sis:hasMeasurementUncertaintyUnivariate ;
                  rdfs:domain sis:Constant ;
                  rdfs:range sis:StandardMU ;
                  skos:prefLabel "has standard measurement uncertainty"@en ,
                                 "hat standard Messunsicherheit"@de .


###  https://ptb.de/sis/isInComplexList
sis:isInComplexList rdf:type owl:ObjectProperty ;
                    rdfs:domain sis:ComplexInList ;
                    rdfs:range sis:ComplexList ;
                    skos:prefLabel "is element of a list of complex values"@en ,
                                   "ist Element in einer Liste komplexer Werte"@de .


###  https://ptb.de/sis/isInListList
sis:isInListList rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( sis:ComplexList
                                                      sis:ListList
                                                      sis:RealList 
                                                     )
                             ] ;
                 rdfs:range sis:ListList ;
                 skos:prefLabel "is element of a list of lists"@en ,
                                "ist Element in einer Liste von Listen"@de .


###  https://ptb.de/sis/isInRealList
sis:isInRealList rdf:type owl:ObjectProperty ;
                 rdfs:domain sis:RealInList ;
                 rdfs:range sis:RealList ;
                 skos:prefLabel "is element of a list of real values"@en ,
                                "ist Element in einer Liste reeller Werte"@de .


#################################################################
#    Data properties
#################################################################

###  https://ptb.de/sis/hasColumnIndex
sis:hasColumnIndex rdf:type owl:DatatypeProperty ;
                   rdfs:domain sis:Covariance ;
                   rdfs:range xsd:positiveInteger ;
                   skos:prefLabel "has column index"@en , "hat Spaltenindex"@de .


###  https://ptb.de/sis/hasCoverageFactor
sis:hasCoverageFactor rdf:type owl:DatatypeProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( sis:ExpandedMU
                                                  sis:MeasurementUncertaintyMultivariate
                                                )
                                  ] ;
                      rdfs:range xsd:double ;
                      rdfs:comment """Scientific decimal format for coverage factor values for uncertainties that must be greater than one or equal to one. Based on type xs:double."""@en ,
                                   """The related type in the XML serialization is a xsd:double restricted by the following regular expression:
                                   \"\\+?(([1-9]\\d*\\.\\d*)|([1-9]\\d*))|NaN\"
                                   """@en ;
                      skos:closeMatch six:kValueType ;
                      skos:definition """points to the coverage factor, i.e., to a factor (greater than or equal to unity)
                                         by which the expanded uncertainty relates to the standard uncertainty"""@en ;
                      skos:prefLabel "has coverage factor"@en ,
                                     "hat Überdeckungsfaktor"@de .


###  https://ptb.de/sis/hasCoverageProbability
sis:hasCoverageProbability rdf:type owl:DatatypeProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( sis:CoverageIntervalMU
                                                       sis:ExpandedMU
                                                       sis:MeasurementUncertaintyMultivariate
                                                     )
                                       ] ;
                           rdfs:range xsd:double ;
                           rdfs:comment """Decimal format for coverage probability values for uncertainties. The value must be within the interval [0,1].
                                           The scientific exponent \"e\" (or \"E\") is not allowed. Based on type xs:double."""@en ,
                                        """The related type in the XML serialization is a xsd:double restricted by the following regular expression:
                                        \"\\+?((0(\\.\\d*)?)|(1(\\.0*)?))|NaN\"
                                        """@en ;
                           skos:closeMatch six:probabilityValueType ;
                           skos:definition """points to the probability (between 0 and 1) ascribed to the event of the actual value
                                              falling within the range asserted by the given uncertainty declaration"""@en ;
                           skos:prefLabel "has coverage probability"@en ,
                                          "hat Deckungswahrscheinlichkeit"@de .


###  https://ptb.de/sis/hasDateTime
sis:hasDateTime rdf:type owl:DatatypeProperty ;
                rdfs:domain sis:QuantityValue ;
                rdfs:range xsd:dateTime ;
                skos:prefLabel "has time stamp"@en ,
                               "hat Zeitstempel"@de .


###  https://ptb.de/sis/hasDistribution
sis:hasDistribution rdf:type owl:DatatypeProperty ;
                    rdfs:domain sis:MeasurementUncertainty ;
                    rdfs:range xsd:string ;
                    skos:definition "points to a descriptor/label of the assumed underlying distribution"@en ;
                    skos:prefLabel "has statistical distribution"@en ,
                                   "hat statistische Verteilung"@de .


###  https://ptb.de/sis/hasIntervalMax
sis:hasIntervalMax rdf:type owl:DatatypeProperty ;
                    rdfs:domain sis:CoverageIntervalMU ;
                    rdfs:range xsd:double ;
                    rdfs:comment """Scientific decimal format for numerical values. Based on type xs:double."""@en ,
                                 """The related type in the XML serialization is a xsd:double restricted by the following regular expression:
                                 \"[-+]?((\\d*\\.\\d+)|(\\d+\\.\\d*)|(\\d+\\.?))([Ee][-+]?\\d+)?|NaN\"
                                 """@en ;
                    skos:closeMatch six:intervalMaxType ;
                    skos:definition "upper boundary of the coverage interval"@en ;
                    skos:prefLabel "has interval maximum"@en ,
                                   "hat Intervallmaximum"@de .


###  https://ptb.de/sis/hasIntervalMin
sis:hasIntervalMin rdf:type owl:DatatypeProperty ;
                    rdfs:domain sis:CoverageIntervalMU ;
                    rdfs:range xsd:double ;
                    rdfs:comment """Scientific decimal format for numerical values. Based on type xs:double."""@en ,
                                 """The related type in the XML serialization is a xsd:double restricted by the following regular expression:
                                 \"[-+]?((\\d*\\.\\d+)|(\\d+\\.\\d*)|(\\d+\\.?))([Ee][-+]?\\d+)?|NaN\"
                                 """@en ;
                    skos:closeMatch six:intervalMin ;
                    skos:definition "lower boundary of the coverage interval"@en ;
                    skos:prefLabel "has interval minimum"@en ,
                                   "hat Intervallminimum"@de .


###  https://ptb.de/sis/hasLabel
sis:hasLabel rdf:type owl:DatatypeProperty ;
             rdfs:domain sis:QuantityValue ;
             rdfs:range xsd:string ;
             skos:prefLabel "has verbose name"@en , "hat ausführlichen Namen"@de .


sis:hasListIndex rdf:type owl:DatatypeProperty ;
                   rdfs:domain sis:RealInList ;
                   rdfs:range xsd:positiveInteger ;
                   skos:prefLabel "has list index"@en , "hat Listenindex"@de .


###  https://ptb.de/sis/hasRowIndex
sis:hasRowIndex rdf:type owl:DatatypeProperty ;
                rdfs:domain sis:Covariance ;
                rdfs:range xsd:positiveInteger ;
                skos:prefLabel "has row index"@en , "hat Zeilenindex"@de .


###  https://ptb.de/sis/hasSignificantDigit
sis:hasSignificantDigit rdf:type owl:DatatypeProperty ;
                        rdfs:domain sis:QuantityValue ;
                        rdfs:range xsd:integer ;
                        rdfs:comment """Property providing information about the rounding range according to the ISO 80000 norm series.
                                     Its value, an integer, reports the position after the decimal separator of the least significant digit of a numerical value."""@en ;
                        skos:closeMatch six:significantDigit ;
                        skos:prefLabel "has number of significant decimal digits"@en ,
                                       "hat Anzahl an signifikante Nachkommastellen"@de .


###  https://ptb.de/sis/hasUnitIdentifier
sis:hasUnitIdentifier rdf:type owl:DatatypeProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( sis:QuantityValue
                                                  sis:Covariance
                                                )
                                  ] ;
                      rdfs:range xsd:string ;
                      rdfs:comment """Unit associated to a quantity, expressed as a xs:string.
                                      Allowed values are chosen from the BIPM SI brochure (9th edition).
                                      The recommended syntax is defined in the SmartCom-D-SI brochure
                                      and is based on the siunitx LaTeX package."""@en ,
                                   """Type described by the following regular expression:
                                   \"((((\\\\kilogram)|(\\\\decibel)|(\\\\degreecelsius)|(\\\\mmhg)|(\\\\minute)|(\\\\hour)|(\\\\day))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?)|(((\\\\deca)|(\\\\hecto)|(\\\\mega)|(\\\\giga)|(\\\\tera)|(\\\\peta)|(\\\\exa)|(\\\\zetta)|(\\\\yotta)|(\\\\deci)|(\\\\centi)|(\\\\milli)|(\\\\micro)|(\\\\nano)|(\\\\pico)|(\\\\femto)|(\\\\atto)|(\\\\zepto)|(\\\\yocto)|(\\\\kibi)|(\\\\mebi)|(\\\\gibi)|(\\\\tebi)|(\\\\pebi)|(\\\\exbi)|(\\\\zebi)|(\\\\yobi))?((\\\\gram))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?)|(((\\\\deca)|(\\\\hecto)|(\\\\kilo)|(\\\\mega)|(\\\\giga)|(\\\\tera)|(\\\\peta)|(\\\\exa)|(\\\\zetta)|(\\\\yotta)|(\\\\centi)|(\\\\milli)|(\\\\micro)|(\\\\nano)|(\\\\pico)|(\\\\femto)|(\\\\atto)|(\\\\zepto)|(\\\\yocto)|(\\\\kibi)|(\\\\mebi)|(\\\\gibi)|(\\\\tebi)|(\\\\pebi)|(\\\\exbi)|(\\\\zebi)|(\\\\yobi))?((\\\\bel))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?)|((\\\\one))|(((\\\\deca)|(\\\\hecto)|(\\\\kilo)|(\\\\mega)|(\\\\giga)|(\\\\tera)|(\\\\peta)|(\\\\exa)|(\\\\zetta)|(\\\\yotta)|(\\\\deci)|(\\\\centi)|(\\\\milli)|(\\\\micro)|(\\\\nano)|(\\\\pico)|(\\\\femto)|(\\\\atto)|(\\\\zepto)|(\\\\yocto)|(\\\\kibi)|(\\\\mebi)|(\\\\gibi)|(\\\\tebi)|(\\\\pebi)|(\\\\exbi)|(\\\\zebi)|(\\\\yobi))?((\\\\metre)|(\\\\second)|(\\\\ampere)|(\\\\kelvin)|(\\\\mole)|(\\\\candela)|(\\\\degree)|(\\\\arcminute)|(\\\\arcsecond)|(\\\\radian)|(\\\\steradian)|(\\\\hertz)|(\\\\newton)|(\\\\pascal)|(\\\\joule)|(\\\\watt)|(\\\\coulomb)|(\\\\volt)|(\\\\farad)|(\\\\ohm)|(\\\\siemens)|(\\\\weber)|(\\\\tesla)|(\\\\henry)|(\\\\lumen)|(\\\\lux)|(\\\\becquerel)|(\\\\sievert)|(\\\\gray)|(\\\\katal)|(\\\\hectare)|(\\\\litre)|(\\\\tonne)|(\\\\electronvolt)|(\\\\dalton)|(\\\\astronomicalunit)|(\\\\neper)|(\\\\percent)|(\\\\ppm)|(\\\\byte)|(\\\\bit)|(\\\\angstrom)|(\\\\bar)|(\\\\atomicunittime)|(\\\\atomicmassunit)|(\\\\barn)|(\\\\clight)|(\\\\electronmass)|(\\\\elementarycharge)|(\\\\naturalunittime)|(\\\\hartree)|(\\\\bohr)|(\\\\nauticalmile)|(\\\\knot)|(\\\\planckbar))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?))+(\\\\per((((\\\\kilogram)|(\\\\decibel)|(\\\\degreecelsius)|(\\\\mmhg)|(\\\\minute)|(\\\\hour)|(\\\\day))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?)|(((\\\\deca)|(\\\\hecto)|(\\\\mega)|(\\\\giga)|(\\\\tera)|(\\\\peta)|(\\\\exa)|(\\\\zetta)|(\\\\yotta)|(\\\\deci)|(\\\\centi)|(\\\\milli)|(\\\\micro)|(\\\\nano)|(\\\\pico)|(\\\\femto)|(\\\\atto)|(\\\\zepto)|(\\\\yocto)|(\\\\kibi)|(\\\\mebi)|(\\\\gibi)|(\\\\tebi)|(\\\\pebi)|(\\\\exbi)|(\\\\zebi)|(\\\\yobi))?((\\\\gram))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?)|(((\\\\deca)|(\\\\hecto)|(\\\\kilo)|(\\\\mega)|(\\\\giga)|(\\\\tera)|(\\\\peta)|(\\\\exa)|(\\\\zetta)|(\\\\yotta)|(\\\\centi)|(\\\\milli)|(\\\\micro)|(\\\\nano)|(\\\\pico)|(\\\\femto)|(\\\\atto)|(\\\\zepto)|(\\\\yocto)|(\\\\kibi)|(\\\\mebi)|(\\\\gibi)|(\\\\tebi)|(\\\\pebi)|(\\\\exbi)|(\\\\zebi)|(\\\\yobi))?((\\\\bel))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?)|((\\\\one))|(((\\\\deca)|(\\\\hecto)|(\\\\kilo)|(\\\\mega)|(\\\\giga)|(\\\\tera)|(\\\\peta)|(\\\\exa)|(\\\\zetta)|(\\\\yotta)|(\\\\deci)|(\\\\centi)|(\\\\milli)|(\\\\micro)|(\\\\nano)|(\\\\pico)|(\\\\femto)|(\\\\atto)|(\\\\zepto)|(\\\\yocto)|(\\\\kibi)|(\\\\mebi)|(\\\\gibi)|(\\\\tebi)|(\\\\pebi)|(\\\\exbi)|(\\\\zebi)|(\\\\yobi))?((\\\\metre)|(\\\\second)|(\\\\ampere)|(\\\\kelvin)|(\\\\mole)|(\\\\candela)|(\\\\degree)|(\\\\arcminute)|(\\\\arcsecond)|(\\\\radian)|(\\\\steradian)|(\\\\hertz)|(\\\\newton)|(\\\\pascal)|(\\\\joule)|(\\\\watt)|(\\\\coulomb)|(\\\\volt)|(\\\\farad)|(\\\\ohm)|(\\\\siemens)|(\\\\weber)|(\\\\tesla)|(\\\\henry)|(\\\\lumen)|(\\\\lux)|(\\\\becquerel)|(\\\\sievert)|(\\\\gray)|(\\\\katal)|(\\\\hectare)|(\\\\litre)|(\\\\tonne)|(\\\\electronvolt)|(\\\\dalton)|(\\\\astronomicalunit)|(\\\\neper)|(\\\\percent)|(\\\\ppm)|(\\\\byte)|(\\\\bit)|(\\\\angstrom)|(\\\\bar)|(\\\\atomicunittime)|(\\\\atomicmassunit)|(\\\\barn)|(\\\\clight)|(\\\\electronmass)|(\\\\elementarycharge)|(\\\\naturalunittime)|(\\\\hartree)|(\\\\bohr)|(\\\\nauticalmile)|(\\\\knot)|(\\\\planckbar))(\\\\tothe\\{-?\\d+(\\.\\d+)?\\})?))+)?$|(^\\|.*$)\"
                                   """@en ;
                      skos:closeMatch six:unit ;
                      skos:prefLabel "has unit identifier"@en , "hat Einheitsidentifikator"@de .


###  https://ptb.de/sis/hasUnitPhaseIdentifier
sis:hasUnitPhaseIdentifier rdf:type owl:DatatypeProperty ;
                           rdfs:domain sis:ComplexPolar ;
                           rdfs:range xsd:string ;
                           rdfs:comment """Type described by the following regular expression:
                                        \"(((\\\\\\\\deca)|(\\\\\\\\hecto)|(\\\\\\\\mega)|(\\\\\\\\giga)|(\\\\\\\\tera)|(\\\\\\\\peta)|(\\\\\\\\exa)|(\\\\\\\\zetta)|(\\\\\\\\yotta)|(\\\\\\\\deci)|(\\\\\\\\centi)|(\\\\\\\\milli)|(\\\\\\\\micro)|(\\\\\\\\nano)|(\\\\\\\\pico)|(\\\\\\\\femto)|(\\\\\\\\atto)|(\\\\\\\\zepto)|(\\\\\\\\yocto)|(\\\\\\\\kibi)|(\\\\\\\\mebi)|(\\\\\\\\gibi)|(\\\\\\\\tebi)|(\\\\\\\\pebi)|(\\\\\\\\exbi)|(\\\\\\\\zebi)|(\\\\\\\\yobi))*\\\\\\\\radian)|(\\\\\\\\metre\\\\\\\\tothe\\\\{0\\\\})|(\\\\\\\\metre\\\\\\\\tothe\\\\{-1\\\\})|(\\\\\\\\degree)|(\\\\\\\\arcminute)|(\\\\\\\\arcsecond)\"
                                        """@en ,
                                        """Unit associated to an angular quantity, associated to a phase angle, expressed as a xs:string.
                                           Allowed values are chosen from the BIPM SI brochure (9th edition).
                                           The recommended syntax is defined in the SmartCom-D-SI brochure
                                           and is based on the siunitx LaTeX package."""@en ;
                           skos:closeMatch six:unitPhase ;
                           skos:prefLabel "has phase unit identifier"@en , "hat Phaseneinheitsidentifikator"@de .


###  https://ptb.de/sis/hasValue
sis:hasValue rdf:type owl:DatatypeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( sis:Constant
                                         sis:Covariance
                                         sis:Real
                                         sis:RealInList
                                       )
                         ] ;
             rdfs:range xsd:double ;
             rdfs:comment """Scientific decimal format for numerical values."""@en ,
                          """The related type in the XML serialization for this datatype property is an xs:double restricted by the following regular expression:
                          \"[-+]?((\\d*\\.\\d+)|(\\d+\\.\\d*)|(\\d+\\.?))([Ee][-+]?\\d+)?|NaN\"
                          """@en ;
